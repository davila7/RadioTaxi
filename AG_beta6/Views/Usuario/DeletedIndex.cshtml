@model IEnumerable<DataLayer6.Usuario>

@{
    ViewBag.Title = "Index";
}

<h2>Usuarios Eliminados</h2>
<p>
    @Html.ActionLink("Usuarios", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Rut)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dig)
        </th>
        <th>
           Nombre Usuario
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            Apellido Paterno
        </th>
        <th>
           Apellido Materno
        </th>
        <th>
            Contraseña
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Perfil.Dsc_Perfil)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Rut)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dig)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_usr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ape_pat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ape_mat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contrasenha)
        </td>
        <td>
           @Html.DisplayFor(modelItem => item.Perfil.Dsc_Perfil)
        </td>
        <td>
            @Html.ActionLink("Restaurar", "Restaurar", new { id=item.Id_Usua }) 
        </td>
    </tr>
}

</table>
