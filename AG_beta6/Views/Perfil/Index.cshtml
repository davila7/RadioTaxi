@model IEnumerable<DataLayer6.Perfil>

@{
    ViewBag.Title = "Index";
}

<h2>Perfiles</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dsc_Perfil)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dsc_Perfil)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_Perfil }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_Perfil }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_Perfil })
        </td>
    </tr>
}

</table>
